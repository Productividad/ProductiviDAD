            <Font size="19.0" />
         </font>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </Label>
      <JFXSpinner fx:id="spinner" progress="0.0" GridPane.columnIndex="1">

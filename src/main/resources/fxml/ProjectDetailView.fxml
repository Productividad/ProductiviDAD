         <padding>
            <Insets right="10.0" />
         </padding>
         <GridPane.margin>
            <Insets bottom="15.0" right="10.0" top="15.0" />
         </GridPane.margin>
      </ScrollPane>
      <Label fx:id="titleProject" styleClass="text-color-card" text="Set titleProject on controller">

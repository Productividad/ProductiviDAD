<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" maxHeight="1.7976931348623157E308" minWidth="200.0" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="cardSection" alignment="CENTER" maxHeight="1.7976931348623157E308" minWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox fx:id="buttonSection" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="15.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="accessButton" minWidth="130.0" mnemonicParsing="false" onAction="#onAccessProject" text="Acceder al proyecto">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="editButton" minWidth="130.0" mnemonicParsing="false" onAction="#onEditProject" text="Editar proyecto">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteButton" minWidth="130.0" mnemonicParsing="false" onAction="#onDeleteProject" text="X">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Button>
               </children>
            </VBox>
            <VBox fx:id="textSection" alignment="CENTER" prefHeight="50.0" prefWidth="100.0" VBox.vgrow="SOMETIMES">
               <children>
                  <Label fx:id="projectTitleLabel" text="gsrsr">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label fx:id="percentageLabel" text="iktyikg">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</fx:root>

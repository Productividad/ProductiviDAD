<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="view" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="50.0" spacing="15.0" stylesheets="@../css/mainStyle.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <ToggleButton fx:id="homeButton" mnemonicParsing="false" onAction="#onHomeButton">
               <styleClass>
                  <String fx:value="side-custom-button" />
                  <String fx:value="home-button" />
               </styleClass>
               <padding>
                  <Insets left="1.0" />
               </padding>
            </ToggleButton>
            <Label fx:id="homeLabel" />
         </children>
         <VBox.margin>
            <Insets left="30.0" right="30.0" />
         </VBox.margin>
      </HBox>
      <Separator>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Separator>
      <ToggleButton fx:id="calendarButton" mnemonicParsing="false" onAction="#onCalendarButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="calendar-button" />
         </styleClass>
      </ToggleButton>
      <ToggleButton fx:id="entryReaderButton" mnemonicParsing="false" onAction="#onEntryReaderButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="reader-button" />
         </styleClass>
      </ToggleButton>
      <ToggleButton fx:id="projectManagerButton" mnemonicParsing="false" onAction="#onProjectManagerButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="project-manager-button" />
         </styleClass>
      </ToggleButton>
      <ToggleButton fx:id="ideasButton" mnemonicParsing="false" onAction="#onIdeasButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="idea-button" />
         </styleClass>
      </ToggleButton>
      <ToggleButton fx:id="balanceManagerButton" mnemonicParsing="false" onAction="#onBalanceManagerButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="expenses-button" />
         </styleClass>
      </ToggleButton>
      <ToggleButton fx:id="timePlannerButton" mnemonicParsing="false" onAction="#onTimePlannerButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="clock-button" />
         </styleClass>
      </ToggleButton>
      <Separator>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Separator>
      <ToggleButton fx:id="toolsButton" mnemonicParsing="false" onAction="#onToolsButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="option-button" />
         </styleClass>
      </ToggleButton>
      <Button mnemonicParsing="false" onAction="#onGithubButton">
         <styleClass>
            <String fx:value="side-custom-button" />
            <String fx:value="github-button" />
         </styleClass>
      </Button>
   </children>
   <styleClass>
      <String fx:value="sideBars-color" />
      <String fx:value="side-menu-bar" />
   </styleClass>
   <padding>
      <Insets bottom="15.0" top="15.0" />
   </padding>
</VBox>
